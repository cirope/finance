<% if @rate_sets.empty? %>
  <%= render 'shared/empty_index', message: t('.empty_html') %>
<% else %>
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th><%= RateSet.human_attribute_name 'name' %></th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @rate_sets.each do |rate_set| %>
        <tr>
          <td><%= rate_set.name %></td>
          <td><%= link_to_show rate_set %></td>
          <td><%= link_to_edit edit_rate_set_path(rate_set) %></td>
          <td><%= link_to_destroy rate_set %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>

<hr />

<%= link_to_new new_rate_set_path, class: 'btn btn-primary' %>
